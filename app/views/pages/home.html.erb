  <h4>Sponsors</h4>
  <!-- Three columns -->

  <div class="row">
    <div class="span3">
    
      <img src="images/grabon.png" alt="GrabOn">
    
      </div>
      <div class="span5">
      
      <img src="images/codechef2.jpg" alt="CodeChef">
     </div>
      </div>
    </div>
    
    <% if current_user %>
    <div class="text-center">
    <a href="index" class="btn btn-info" role="button">Go to treasure hunt</a>
    </div>
    <%end %>
</section>

